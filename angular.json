{
   "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
   "version": 1,
   "newProjectRoot": "projects",
   "projects": {
      "X4": {
         "root": "",
         "sourceRoot": "src",
         "projectType": "application",
         "prefix": "app",
         "schematics": {},
         "architect": {
            "build": {
               "builder": "@angular-devkit/build-angular:browser",
               "options": {
                  "outputPath": "dist/X4",
                  "index": "src/index.html",
                  "main": "src/main.ts",
                  "polyfills": "src/polyfills.ts",
                  "tsConfig": "src/tsconfig.app.json",
                  "assets": [
                     "src/favicon.ico",
                     "src/assets"
                  ],
                  "styles": [
                     "src/assets/styles/dx.common.css",
                     "src/assets/styles/generic.light.custom.css",
                     "node_modules/bootstrap/dist/css/bootstrap.min.css",
                     "node_modules/font-awesome/css/font-awesome.min.css",
                     "src/styles.scss"
                  ],
                  "scripts": []
               },
               "configurations": {
                  "zh-Hans": {
                     "fileReplacements": [
                        {
                           "replace": "src/environments/environment.ts",
                           "with": "src/environments/environment.prod.ts"
                        }
                     ],
                     "optimization": true,
                     "outputHashing": "all",
                     "sourceMap": false,
                     "extractCss": true,
                     "namedChunks": false,
                     "extractLicenses": true,
                     "vendorChunk": false,
                     "buildOptimizer": true,
                     "budgets": [
                        {
                           "type": "initial",
                           "maximumWarning": "2mb",
                           "maximumError": "5mb"
                        }
                     ],
                     "aot": true,
                     "outputPath": "dist/X4/zh-Hans/",
                     "baseHref": "/zh-Hans/",
                     "i18nFile": "src/i18n/messages.zh-Hans.xlf",
                     "i18nFormat": "xlf",
                     "i18nLocale": "zh-Hans",
                     "i18nMissingTranslation": "warning"
                  },
                  "production": {
                     "fileReplacements": [
                        {
                           "replace": "src/environments/environment.ts",
                           "with": "src/environments/environment.prod.ts"
                        }
                     ],
                     "optimization": true,
                     "outputHashing": "all",
                     "sourceMap": false,
                     "extractCss": true,
                     "namedChunks": false,
                     "aot": true,
                     "extractLicenses": true,
                     "vendorChunk": false,
                     "buildOptimizer": true,
                     "budgets": [
                        {
                           "type": "initial",
                           "maximumWarning": "2mb",
                           "maximumError": "5mb"
                        }
                     ]
                  }
               }
            },
            "serve": {
               "builder": "@angular-devkit/build-angular:dev-server",
               "options": {
                  "browserTarget": "X4:build"
               },
               "configurations": {
                  "zh-Hans": {
                     "browserTarget": "X4:build:zh-Hans"
                  },
                  "production": {
                     "browserTarget": "X4:build:production"
                  }
               }
            },
            "extract-i18n": {
               "builder": "@angular-devkit/build-angular:extract-i18n",
               "options": {
                  "browserTarget": "X4:build"
               }
            },
            "lint": {
               "builder": "@angular-devkit/build-angular:tslint",
               "options": {
                  "tsConfig": [
                     "src/tsconfig.app.json",
                     "src/tsconfig.spec.json"
                  ],
                  "exclude": [
                     "**/node_modules/**"
                  ]
               }
            }
         }
      }
   },
   "defaultProject": "X4",
   "schematics": {
      "@schematics/angular:component": {
         "prefix": "app",
         "spec": false,
         "inlineStyle": true
      },
      "@schematics/angular:class": {
         "spec": false
      },
      "@schematics/angular:directive": {
         "prefix": "app",
         "spec": false
      },
      "@schematics/angular:guard": {
         "spec": false
      },
      "@schematics/angular:module": {
         "spec": false
      },
      "@schematics/angular:pipe": {
         "spec": false
      },
      "@schematics/angular:service": {
         "spec": false
      }
   }
}
